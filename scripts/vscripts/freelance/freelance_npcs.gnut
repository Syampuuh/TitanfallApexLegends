bool isInitialized = false
bool isNetworkingRegistered = false


void function FreelanceNPCs_Init()
{
	if ( isInitialized )
	{
		#if DEV
			printf( "Freelance NPCs System is already initialized" )
		#endif
		return
	}
	isInitialized = true
#if SERVER

	               

	                                              

	                                              
	                                                        

	  
	                                           
	                                         
#endif          

#if CLIENT
	AddCreateCallback( "npc_spectre", OnNPCCreatedClient )
#endif         

                   
                        
             
                                    
                                      

               
                                      
                                        

           
                                  
                                    

               
                                      
                                                                         
                                                                         
                                                                          
                                                                          

                      
         
                                
                                       
                                            

          
                                    
                                      

                  

                     
        
                               
                                 
                                             
                                            

                      
                                        
                                        
                                        
                                        

            
                                                          
                                       

   
                            
                        
