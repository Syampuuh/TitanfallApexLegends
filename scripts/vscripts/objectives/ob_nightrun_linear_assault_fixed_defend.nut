void function OnGenericPanelSpawn( entity panel )
{
	if ( panel.GetScriptName() != GENERIC_PANEL_SCRIPTNAME )
		return

	#if SERVER
		                     
		                                         
		                 
		                                                                                                    
		                                               
		                                                 
		                                           
	#elseif CLIENT
		AddEntityCallback_GetUseEntOverrideText( panel, ExtendedUseTextOverride )
	#endif

	AddCallback_OnUseEntity_ClientServer( panel, OnGenericPanelUse )
}
            


        
string function GetUsePromptsForModel( entity panel )
{
	asset modelAsset = panel.GetModelName()
	string useString
	switch( modelAsset )
	{
		case GetObjectiveAsset_Model( "MODEL_GENERIC_PANEL" ):
			return "#FREELANCE_GENERIC_PANEL_USESTRING"
		case GetObjectiveAsset_Model( "MODEL_DEATH_TOTEM" ):
			return "#FREELANCE_DEATH_TOTEM_USESTRING"
		default:
			Assert( 0, "Unhandled modelAsset " + modelAsset )
	}

	unreachable
}
            



#if CLIENT
string function GetUseLoopSoundForModel( entity panel )
{
	asset modelAsset = panel.GetModelName()
	string useString
	switch( modelAsset )
	{
		case GetObjectiveAsset_Model( "MODEL_GENERIC_PANEL" ):
			return "SQ_Panel_Use_Loop"
		case GetObjectiveAsset_Model( "MODEL_DEATH_TOTEM" ):
			return "SQ_Totem_Use_Loop"
		default:
			return "SQ_Item_Use_Loop"
	}

	unreachable
}
#endif



#if CLIENT
string function GetUseSuccessSoundForModel( entity panel )
{
	asset modelAsset = panel.GetModelName()
	string useString
	switch( modelAsset )
	{
		case GetObjectiveAsset_Model( "MODEL_GENERIC_PANEL" ):
			return "SQ_Panel_Use_Complete"
		case GetObjectiveAsset_Model( "MODEL_DEATH_TOTEM" ):
			return "SQ_Totem_Use_Complete"
		default:
			return "SQ_Item_Use_Complete"
	}

	unreachable
}
#endif


        
string function GetUsePromptsLongForModel( entity panel )
{
	asset modelAsset = panel.GetModelName()
	string useString
	switch( modelAsset )
	{
		case GetObjectiveAsset_Model( "MODEL_GENERIC_PANEL" ):
			return "#FREELANCE_GENERIC_PANEL_LONG_USESTRING"
		case GetObjectiveAsset_Model( "MODEL_DEATH_TOTEM" ):
			return "#FREELANCE_DEATH_TOTEM_LONG_USESTRING"
		default:
			Assert( 0, "Unhandled modelAsset " + modelAsset )
	}

	unreachable
}
            




        
void function OnGenericPanelUse( entity panel, entity player, int useInputFlags )
{
	if ( useInputFlags & USE_INPUT_LONG )
	{
		thread GenericPanelUseThink( panel, player )
	}
}
            



        
void function GenericPanelUseThink( entity ent, entity playerUser )
{
	ExtendedUseSettings settings
	#if CLIENT
		settings.loopSound = GetUseLoopSoundForModel( ent )
		settings.successSound = GetUseSuccessSoundForModel( ent )
		settings.displayRui = $"ui/extended_use_hint.rpak"
		settings.displayRuiFunc = DefaultExtendedUseRui
		settings.icon = $""
		settings.hint = GetUsePromptsLongForModel( ent )
	#elseif SERVER
		                                               
		                             
		                               
	#endif

	settings.duration = GENERIC_PANEL_USE_TIME
	settings.requireMatchingUseEnt = true

	ent.EndSignal( "OnDestroy" )

	waitthread ExtendedUse( ent, playerUser, settings )

}
            


